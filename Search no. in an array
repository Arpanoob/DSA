
package Array;
import java.util.*;
public class Searchno {
    public static void main(String args[]){
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int arr[]=new int[n];
    for(int i=0;i<n;i++){
        arr[i]=sc.nextInt();
    }
    int x=sc.nextInt();
    for(int j=0;j<n;j++){
       if(arr[j]==x){
        System.out.println(j);
       }
}}}

